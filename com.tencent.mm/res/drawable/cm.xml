<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c8" />
            <solid android:color="@color/qa" />
            <stroke android:width="@dimen/c9" android:color="@color/cc" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c8" />
            <solid android:color="@color/qa" />
            <stroke android:width="@dimen/c9" android:color="@color/ce" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/c8" />
            <solid android:color="@color/qa" />
            <stroke android:width="@dimen/c9" android:color="@color/cd" />
        </shape>
    </item>
</selector>